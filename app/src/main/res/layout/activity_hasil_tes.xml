                            android:textColor="@color/white"
                            android:textSize="8pt"/>

                        <EditText
                            android:id="@+id/edtJamTidur"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="3dp"
                            android:layout_marginRight="15dp"
                            android:background="@color/white"
                            android:clickable="true"
                            android:ems="10"
                            android:enabled="false"
                            android:focusable="false"
                            android:hint="Masukkan jam tidur Anda"
                            android:inputType="text"
                            android:paddingLeft="10dp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"
                            android:textColor="@color/black"
                            android:textSize="7pt" />

                        <TextView
                            android:id="@+id/txvJamBangun"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="15dp"
                            android:layout_marginRight="15dp"
                            android:text="Silahkan tuliskan jam bangun tidur Anda! "
                            android:textColor="@color/white"
                            android:textSize="8pt"/>

                        <EditText
                            android:id="@+id/edtJamBangun"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="3dp"
                            android:layout_marginRight="15dp"
                            android:background="@color/white"
                            android:clickable="true"
                            android:ems="10"
                            android:enabled="false"
                            android:focusable="false"
                            android:hint="Masukkan jam bangun Anda"
                            android:inputType="text"
                            android:paddingLeft="10dp"
                            android:paddingTop="8dp"
                            android:paddingBottom="8dp"
                            android:textColor="@color/black"
                            android:textSize="7pt" />

                        <TextView
                            android:id="@+id/txvNafsu"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="17dp"
                            android:layout_marginRight="15dp"
                            android:text="Apakah Anda mengalami perubahan nafsu makan saat ini?"
                            android:textColor="@color/white"
                            android:textSize="8pt"/>

                        <RadioGroup
                            android:id="@+id/rdgYNMakan"
                            android:layout_width="200dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="17dp"
                            android:layout_marginRight="10dp"
                            android:orientation="vertical">

                            <RadioButton
                                android:id="@+id/rdbYMakan"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="-8dp"
                                android:clickable="false"
                                android:focusable="false"
                                android:scaleX="0.9"
                                android:scaleY="0.9"
                                android:text="Ya"
                                android:textSize="7pt"
                                android:textColor="@color/white"/>

                            <RadioButton
                                android:id="@+id/rdbNMakan"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="-8dp"
                                android:layout_marginTop="-15dp"
                                android:clickable="false"
                                android:focusable="false"
                                android:scaleX="0.9"
                                android:scaleY="0.9"
                                android:text="Tidak"
                                android:textSize="7pt"
                                android:textColor="@color/white"/>
                        </RadioGroup>

                        <TextView
                            android:id="@+id/txvMakan"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="17dp"
                            android:layout_marginRight="15dp"
                            android:text="Seberapa sering Anda melewatkan jadwal makan karena mengalami tekanan/perasaan yang tidak baik?"
                            android:textColor="@color/white"
                            android:textSize="8pt"/>

                        <SeekBar
                            android:id="@+id/skbLewatMakan"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginRight="15dp"
                            android:clickable="false"
                            android:focusable="false"
                            android:progressTint="@color/bg"
                            android:progressBackgroundTint="#FFFFFF"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="-12dp"
                            android:layout_marginBottom="8dp"
                            android:orientation="horizontal"
                            android:padding="16dp">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Tidak pernah"
                                android:textColor="@color/white"
                                android:textSize="7pt"/>

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="end"
                                android:text="Sangat sering"
                                android:textColor="@color/white"
                                android:textSize="7pt"/>
                        </LinearLayout>
                    </LinearLayout>
                </ScrollView>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>